  <DataSets>
    <DataSet Name="UserActivity">
      <Query>
        <DataSourceName>DBDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@From">
            <Value>=Parameters!From.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@To">
            <Value>=Parameters!To.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserID">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @sql NVARCHAR(4000), @params nvarchar(4000)  

SELECT @sql = 'SELECT CONVERT(VARCHAR(10), COALESCE(rc.ModifiedDate, CreatedDate), 101) Date, 1 As ID, ''Cases Created'' Status  FROM Record_Case rc
WHERE 1=1'

IF @From IS NOT NULL
	SELECT @sql = @sql + ' AND (rc.CreatedDate &gt;= @From)'

IF @To IS NOT NULL
	SELECT @sql = @sql + ' AND (rc.CreatedDate &lt;= DATEADD(DAY,1,@To))'

IF @UserID IS NOT NULL
	SELECT @sql = @sql + ' AND (rc.UserID = @UserID)'

SELECT @sql = @sql + ' UNION ALL '

SELECT @sql = @sql + 'SELECT CONVERT(VARCHAR(10), COALESCE(br.ModifiedDate, rh.CreatedDate), 101) Date, 1 AS ID, rst.Name Status  FROM Batch_Record br
left outer join Record_History rh on rh.RecordID = br.ID 
LEFT OUTER JOIN Record_Status_Type rst ON rst.ID = rh.RecordStatusID
WHERE 1=1'

IF @From IS NOT NULL
	SELECT @sql = @sql + ' AND (rh.CreatedDate &gt;= @From)'

IF @To IS NOT NULL
	SELECT @sql = @sql + ' AND (rh.CreatedDate &lt;= DATEADD(DAY,1,@To))'

IF @UserID IS NOT NULL
	SELECT @sql = @sql + ' AND (rh.UserID = @UserID)'

SELECT @params = N'@From Date = NULL, @To Date = NULL, @UserID INT = NULL'
print @sql
EXEC sp_executesql @sql, @params, @From, @To, @UserID</CommandText>
      </Query>
      <Fields>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
